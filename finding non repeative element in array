/*
2. Return the element which is non repeating
Input : [10, 22, 10, 1, 22 ]
Output : 1
*/
public class HelloWorld{

     public static void main(String []args){
        
        int [] array ={10,22,10,99,22};
        
        int bit =0;
        
        for(int i=0;i<array.length;i++){
            bit=bit^array[i];
            
        }
        System.out.println(bit);
     }
}
